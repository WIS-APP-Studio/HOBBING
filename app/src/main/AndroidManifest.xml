<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hobbing" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:usesCleartextTraffic="true"

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HOBBING">
        <activity android:name="com.wisappstudio.hobbing.activity.LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.wisappstudio.hobbing.activity.IntroActivity" />
        <activity android:name="com.wisappstudio.hobbing.activity.SignInActivity"/>
        <activity android:name="com.wisappstudio.hobbing.activity.SignUpActivity"/>
        <activity android:name="com.wisappstudio.hobbing.activity.MainActivity"/>
        <activity android:name="com.wisappstudio.hobbing.activity.WritePostActivity"/>
    </application>

</manifest>